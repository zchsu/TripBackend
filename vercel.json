{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "buildCommand": "sh ./build.sh"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "env": {
      "PYTHONPATH": ".",
      "PLAYWRIGHT_BROWSERS_PATH": "0"
    }
  }